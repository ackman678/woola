{
  "name": "woola",
  "version": "1.0.0",
  "description": "A friendly beast to help you render, build, and publish markdown notebook content",
  "main": "./lib/mdbuild.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node ./lib/mdbuild.js",
    "start": "node ./lib/server.js",
    "postinstall": "binalias='alias woola=\"node '$(npm prefix)'/lib/server.js\"'; echo $binalias >> ~/.bashrc; source ~/.bashrc"
  },
  "bin": {
    "woola": "./lib/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ackmanlab/woola.git"
  },
  "keywords": [
    "markdown-it",
    "commonmark",
    "jekyll",
    "notebook",
    "blog",
    "pandoc",
    "pdf"
  ],
  "author": "James Ackman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ackmanlab/woola/issues"
  },
  "homepage": "https://github.com/ackmanlab/woola#readme",
  "dependencies": {
    "browser-sync": "^2.18.13",
    "chokidar": "^1.7.0",
    "front-matter": "^2.1.2",
    "html-pdf": "^2.1.0",
    "markdown-it": "^8.3.1",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "git+https://github.com/ackmanlab/markdown-it-footnote.git",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "micromatch": "^3.0.4"
  }
}
